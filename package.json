{
  "name": "project",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "type": "commonjs",
  "scripts": {
    "build:frontend": "webpack --config src/frontend/webpack.config.js",
    "watch:frontend": "webpack --config src/frontend/webpack.config.js --watch",
    "start": "npx ts-node start-in-terminal.ts",
    "build": "rm -rf ./dist && rm -rf ./static && yarn run build:frontend && yarn run tsc && yarn run move-assets",
    "oneshot": "npx ts-node start-oneshot.ts",
    "server": "yarn run tsc && node dist/backend/server.js",
    "frontend": "yarn run tsc --project src/frontend/tsconfig.json && yarn run move-assets",
    "move-assets": "cp src/frontend/index.html static/ && mkdir -p static/styles && find src/frontend -type f -name \"*.css\" -exec cp {} static/styles/ \\;",
    "web": "yarn run build && node dist/backend/server.js",
    "open": "node dist/open-frontend.js"
  },
  "author": "vincent.audibert@whoz.com",
  "license": "MIT",
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.17",
    "nodemon": "^2.0.7",
    "opn-cli": "^5.0.0",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.2",
    "webpack": "^5.94.0",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "@google/generative-ai": "^0.19.0",
    "@inquirer/prompts": "^5.1.0",
    "@types/yargs": "^17.0.32",
    "axios": "^1.7.2",
    "chalk": "^4.1.2",
    "express": "^4.17.1",
    "glob": "^10.4.1",
    "openai": "^4.60.0",
    "rxjs": "^7.8.1",
    "yaml": "^2.4.5",
    "yargs": "^17.7.2"
  }
}
