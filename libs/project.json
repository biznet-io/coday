{
  "name": "coday",
  "$schema": "../node_modules/nx/schemas/project-schema.json",
  "projectType": "library",
  "targets": {
    "build-terminal": {
      "executor": "@nx/esbuild:esbuild",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/start",
        "outputFileName": "start-in-terminal.js",
        "main": "libs/start-in-terminal.ts",
        "tsConfig": "tsconfig.base.json",
        "platform": "node",
        "format": ["esm"],
        "assets": []
      }
    },
    "start-in-terminal": {
      "executor": "nx:run-commands",
      "dependsOn": ["coday:build-terminal"],
      "options": {
        "commands": ["node dist/start/start-in-terminal.js"],
        "parallel": false
      }
    }
  }
}
